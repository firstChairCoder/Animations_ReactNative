{
    "extends": "@react-native-community",
    "plugins": ["import" ,"prettier"],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    },
    "rules": {
      "quotes": "off",
      "react-hooks/exhaustive-deps": "off",
      "comma-dangle": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
}
  

  